@model High_TMDT.Data.LoaiSanPham

@{
    ViewData["Title"] = "Detail_loaisp";
}
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css">

<div class="container mt-3">
    <h4 class="fw-bold">LoaiSanPham</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <span class="fw-bold">@Html.DisplayNameFor(model => model.TenLoaiSp)</span>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TenLoaiSp)
        </dd>
    </dl>
</div>

<div class="container mt-3">
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model?.IdLoaiSp">Edit</a>
    <a class="btn btn-secondary" asp-action="Index">Trờ về</a>
</div>
